{
    "name": "dvlpp/sharp",
    "description": "A kind of CMS for developers who hate CMS",
    "authors": [
        {
            "name": "Philippe Lonchampt",
            "email": "philippelonchampt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "illuminate/html": "~5.0",
        "intervention/image": "2.*",
        "michelf/php-markdown": "1.4.*@dev",
        "mustache/mustache": "~2.6.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers"
        ],
        "files": [
            "src/helpers/sharp_cms.php",
            "src/helpers/sharp_internal.php"
        ],
        "psr-4": {
            "Dvlpp\\Sharp\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
